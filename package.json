{
  "name": "ts-in-egg",
  "version": "1.0.0",
  "description": "egg 中使用 ts",
  "private": true,
  "egg": {
    "typescript": true,
    "declarations": true
  },
  "dependencies": {
    "egg": "^2.15.1",
    "egg-scripts": "^2.11.0"
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^5.26.0",
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "egg-bin": "^4.11.0",
    "egg-ci": "^1.11.0",
    "egg-mock": "^3.21.0",
    "egg-ts-helper": "^1.30.3",
    "eslint": "^5.13.0",
    "eslint-config-egg": "^7.1.0",
    "typescript-eslint": "0.0.1-alpha.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-ts-in-egg",
    "stop": "egg-scripts stop --title=egg-server-ts-in-egg",
    "dev": "egg-bin dev --dts",
    "debug": "egg-bin debug",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov",
    "autod": "autod"
  },
  "ci": {
    "version": "10"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "singing tang",
  "license": "MIT"
}
